<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="manage.dao.SessionMapper">
    <select id="queryUserNameBySession" resultType="java.lang.String">
        select user_name from session
        where `session`=#{session}
    </select>
    <insert id="saveSession">
        insert into session(`session`,user_name) VALUE (#{session},#{userName})
    </insert>
</mapper>